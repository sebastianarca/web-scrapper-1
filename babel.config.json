{
    "presets": [
        ["@babel/preset-env", {
            "targets":{ "esmodules": false },
            "modules": "commonjs",
            "forceAllTransforms": true
        }]
    ],
    "plugins": [
        ["@babel/plugin-transform-runtime", {
            "absoluteRuntime": false,
            "corejs": false,
            "helpers": true,
            "regenerator": true,
            "useESModules": false
          }]
    ]
  }